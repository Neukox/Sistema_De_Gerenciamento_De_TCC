# Database URL para PostgreSQL local
# Ajuste a URL conforme sua configuração local
DATABASE_URL="postgresql://usuario:senha@localhost:5432/sistema_tcc"

# Configurações de ambiente
NODE_ENV=development
JWT_SECRET=seu_jwt_secret_aqui

# Se você estiver usando PostgreSQL com Docker, use:
# DATABASE_URL="postgresql://postgres:password@localhost:5432/sistema_tcc"
